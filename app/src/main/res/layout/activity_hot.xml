<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFFFF"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawer_layout">

    <LinearLayout
        android:clipChildren="false"
        android:clipToPadding="false"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:id="@+id/r153vdj63i04"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingBottom="-41dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/rvceb8aus34"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFFFF"
                    android:paddingVertical="8dp"
                    android:paddingRight="16dp"
                    android:layout_marginBottom="11dp"
                    android:elevation="4dp"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/rhfraehdezvc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="35dp"
                        android:layout_marginStart="27dp"
                        android:clipChildren="false"
                        android:clipToPadding="false"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/sidebarTrigger"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:background="@drawable/menu"
                            android:layout_marginEnd="7dp"
                            android:layout_marginTop="10dp"/>

                        <TextView
                            android:id="@+id/rk53q8sfvcuq"
                            android:textColor="#000000"
                            android:textSize="20sp"
                            android:layout_marginTop="16dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Shop"/>

                        <LinearLayout
                            android:id="@+id/rewkk6e2ltvh"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:clipChildren="false"
                            android:clipToPadding="false"
                            android:orientation="vertical"/>

                        <ImageView
                            android:id="@+id/rpp5hxqkonal"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginEnd="15dp"
                            android:src="@drawable/search"
                            android:scaleType="fitXY"/>

                        <ImageView
                            android:id="@+id/r2t9k0n64q7m"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginTop="10dp"
                            android:src="@drawable/bag"
                            android:scaleType="fitXY"/>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/roz7zb1enne"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:layout_marginStart="28dp"
                        android:clipChildren="false"
                        android:clipToPadding="false"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/men"
                            android:textColor="#000000"
                            android:textSize="20sp"
                            android:layout_marginEnd="4dp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Men"/>

                        <TextView
                            android:id="@+id/women"
                            android:textColor="#000000"
                            android:textSize="20sp"
                            android:layout_marginEnd="59dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Women"/>

                        <TextView
                            android:id="@+id/kids"
                            android:textColor="#000000"
                            android:textSize="20sp"
                            android:layout_marginEnd="57dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Kids"/>

                        <TextView
                            android:id="@+id/rzsx9b0wjbsd"
                            android:textColor="#000000"
                            android:textSize="20sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Hot"/>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/rcu0wnxx6vew"
                        android:layout_width="41dp"
                        android:layout_height="1dp"
                        android:background="#FF000000"
                        android:layout_marginStart="360dp"
                        android:clipChildren="false"
                        android:clipToPadding="false"
                        android:orientation="vertical"/>
                </LinearLayout>

                <ImageView
                    android:id="@+id/rtvovv171i3c"
                    android:layout_width="129dp"
                    android:layout_height="39dp"
                    android:layout_marginBottom="13dp"
                    android:layout_marginHorizontal="27dp"
                    android:src="@drawable/dragoball"
                    android:scaleType="fitXY"/>

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/dragonBall"
                    android:layout_width="match_parent"
                    android:layout_height="253dp"
                    android:layout_marginBottom="39dp"
                    android:layout_marginHorizontal="5dp"
                    android:src="@drawable/framedb"
                    android:scaleType="fitXY"/>

                <TextView
                    android:id="@+id/r98keoowpee5"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:layout_marginBottom="12dp"
                    android:layout_marginStart="24dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="T-Shirt Unisex"/>

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/TShirtUnisex"
                    android:layout_width="match_parent"
                    android:layout_height="142dp"
                    android:layout_marginBottom="40dp"
                    android:layout_marginHorizontal="5dp"
                    android:src="@drawable/unisex"
                    android:scaleType="fitXY"/>

                <TextView
                    android:id="@+id/r4sbc3we87p8"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:layout_marginBottom="16dp"
                    android:layout_marginStart="6dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Sepatu Unisex"/>

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rzkswb5x3ujo"
                    android:layout_width="match_parent"
                    android:layout_height="161dp"
                    android:layout_marginHorizontal="5dp"
                    android:src="@drawable/sepatuunisex"
                    android:scaleType="fitXY"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <include layout="@layout/sidebar"
        android:layout_width="280dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"/>
</androidx.drawerlayout.widget.DrawerLayout>
